{
  "name": "greenTravel",
  "version": "1.6.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "start-dev-client": "npx expo start --dev-client",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "test": "jest",
    "lint": "yarn run eslint './src/**/*.{ts,tsx,js}' && yarn run tsc",
    "lint-fix": "yarn run eslint --fix './src/**/*.{ts,tsx,js}'",
    "test-clean": "yarn jest --clear-cache && yarn test --slowTestThreshold=0.2",
    "precommit": "yarn run eslint './src/**/*.{ts,tsx,js}' && yarn run tsc",
    "update-svg": "node scripts/update-svg/update-svg.js",
    "build-ios": "eas build --profile production --platform ios --local",
    "build-dev-client-ios": "eas build --profile development --platform ios --local",
    "postinstall": "patch-package && yarn husky install",
    "doctor": "npx expo-doctor",
    "upgrade-packages": "yarn upgrade-interactive --latest --ignore-scripts",
    "prebuild": "npx expo prebuild --clean",
    "prebuild-ios": "npx expo prebuild -p ios --clean",
    "prebuild-android": "npx expo prebuild -p android --clean"
  },
