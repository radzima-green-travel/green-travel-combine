{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "isolatedModules": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "noImplicitAny": false,
    "module": "nodenext",
    "target": "ESNext",
    "moduleResolution": "nodenext",
    "paths": {
      "components/*": [
        "./src/components/*"
      ],
      "atoms/*": [
        "./src/components/atoms/*"
      ],
      "atoms": [
        "./src/components/atoms"
      ],
      "containers/*": [
        "./src/components/containers/*"
      ],
      "containers": [
        "./src/components/containers"
      ],
      "assets": [
        "./src/assets"
      ],
      "assets/*": [
        "./src/assets/*"
      ],
      "screens/*": [
        "./src/components/screens/*"
      ],
      "screens": [
        "./src/components/screens"
      ],
      "core": [
        "./src/core"
      ],
      "core/*": [
        "./src/core/*"
      ],
      "services": [
        "./src/services"
      ],
      "services/*": [
        "./src/services/*"
      ],
      "molecules": [
        "./src/components/molecules"
      ],
      "molecules/*": [
        "./src/components/molecules/*"
      ],
      "organisms": [
        "./src/components/organisms"
      ],
      "organisms/*": [
        "./src/components/organisms/*"
      ],
      "mock": [
        "./src/mock"
      ],
      "selectors": [
        "./src/core/selectors"
      ],
      "navigation": [
        "./src/navigation"
      ],
      "navigation/*": [
        "./src/navigation/*"
      ],
      "api": [
        "./src/api"
      ],
      "api/*": [
        "./src/api/*"
      ],
      "storage": [
        "./src/storage"
      ],
      "storage/*": [
        "./src/storage/*"
      ],
      "preview": ["./src/components/atoms/Preview/Preview"]
    },
  },
}
